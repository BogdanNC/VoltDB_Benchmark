<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<deployment>
    <cluster sitesperhost="2" kfactor="0"/>
    <partition-detection>
        <snapshot/>
    </partition-detection>
    <admin-mode adminstartup="false"/>
    <heartbeat/>
    <httpd enabled="true">
        <jsonapi enabled="true"/>
    </httpd>
    <snapshot enabled="false"/>
    <commandlog enabled="false">
        <frequency/>
    </commandlog>
    <systemsettings>
        <temptables maxsize="4500"/>
        <snapshot/>
        <elastic/>
        <query timeout="30000"/>
        <resourcemonitor>
            <memorylimit size="26"/>
        </resourcemonitor>
    </systemsettings>
    <security/>
</deployment>
